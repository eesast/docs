{
  "name": "eesast-docs",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "typecheck": "tsc --noUnusedLocals",
    "prettier": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md,mdx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.f48d435ce",
    "@docusaurus/preset-classic": "2.0.0-alpha.f48d435ce",
    "@docusaurus/theme-live-codeblock": "2.0.0-alpha.f48d435ce",
    "@easyops-cn/docusaurus-search-local": "0.14.2",
    "@mdx-js/react": "1.6.22",
    "antd": "4.10.1",
    "clsx": "1.1.1",
    "lint-staged": "10.5.3",
    "nodejieba": "2.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-alpha.f48d435ce",
    "@tsconfig/docusaurus": "1.0.2",
    "@types/react": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "all-contributors-cli": "6.19.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "husky": "5.0.6",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  }
}
