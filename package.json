{
  "name": "eesast-docs",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "typecheck": "tsc --noUnusedLocals",
    "prettier": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md,mdx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.fd17476c3",
    "@docusaurus/preset-classic": "2.0.0-alpha.fd17476c3",
    "@docusaurus/theme-live-codeblock": "2.0.0-alpha.fd17476c3",
    "@easyops-cn/docusaurus-search-local": "0.15.1",
    "@mdx-js/react": "1.6.22",
    "antd": "4.16.5",
    "clsx": "1.1.1",
    "lint-staged": "11.0.0",
    "nodejieba": "2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-katex": "5.0.0",
    "remark-math": "4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-alpha.fd17476c3",
    "@tsconfig/docusaurus": "1.0.2",
    "@types/react": "17.0.11",
    "@types/react-helmet": "6.1.1",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "all-contributors-cli": "6.20.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "6.0.0",
    "prettier": "2.3.2",
    "typescript": "4.3.4"
  }
